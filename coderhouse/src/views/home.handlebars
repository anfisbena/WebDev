<div>
{{#each product}}
  <div>
    {{#each this.thumbnails}}
      <img src={{this}} alt="productItem">
    {{/each}}
    <h2>{{this.title}}</h2><p>NKU:{{this.code}}</p>
    <p>{{this.description}}</p>
    <h2>${{this.price}}</h2>
  </div>
{{/each}}
</div>
<script src="/socket.io/socket.io.js"></script>
